.containor {
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.containor .deviceInfo {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  margin: 30rpx;
}
.containor .deviceInfo .device-pic {
  max-width: 250rpx;
  max-height: 250rpx;
  margin: 30rpx;
}
.containor .deviceInfo .device-text {
  margin: 20rpx;
  font-size: 20px;
}
.containor .deviceInfo .device-place {
  font-size: 15px;
  color: grey;
}
.containor .wifi-containor {
  margin: 30rpx;
}
.containor .wifi-containor .wifi {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.containor .wifi-containor .wifi .wifi-logo {
  max-width: 100rpx;
  max-height: 100rpx;
  margin: 20rpx;
}
.containor .wifi-containor .wifi .wifi-text {
  font-size: 20px;
}
.containor .button-containor .pic-button {
  display: flex;
  align-items: center;
}
.containor .button-containor .pic-button .col2Image {
  max-width: 200rpx;
  max-height: 200rpx;
}
.containor .delete-pic {
  height: 50rpx;
  margin-top: 30rpx;
}